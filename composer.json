{
	"name" : "czhujer/slim-auth",
	"description" : "Authorization and authentication for the Slim Framework using ZF2 Authentication and Acl components",
	"homepage" : "https://github.com/marcelbonnet/slim-auth",
	"support" : {
		"issues" : "https://github.com/marcelbonnet/slim-auth/issues",
		"source" : "https://github.com/marcelbonnet/slim-auth"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Marcel Bonnet",
			"role" : "developer",
			"homepage" : "https://github.com/marcelbonnet"
		}
	],
	"keywords" : [
		"auth",
		"authorization",
		"authentication",
		"slim",
		"slim framework",
		"zend",
		"zend framework",
		"zf2"
	],
	"require" : {
		"php" : "~8.1.0 || ~8.2.0 || ~8.3.0",
		"laminas/laminas-authentication" : "~2.16",
		"laminas/laminas-permissions-acl" : "~2.16",
		"laminas/laminas-session" : "~2.20",
		"laminas/laminas-config" : "~3.9",
		"laminas/laminas-ldap" : "~2.18",
		"doctrine/orm" : "^2.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "4.*",
		"slim/slim" : "^3.0",
		"jeremykendall/debug-die" : "0.0.1.*"
	},
	"autoload" : {
		"psr-4" : {
			"h-software\\Slim\\Auth\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"h-software\\Slim\\Auth\\Tests\\" : "tests/"
		}
	},
	"suggest" : {
	},
	"conflict": {
        "marcelbonnet/slim-auth": "^2.0"
    }
}
